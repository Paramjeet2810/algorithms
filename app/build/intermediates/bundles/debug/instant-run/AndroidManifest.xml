<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.suvariyaraj.algorithms"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/app_icon3"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.suvariyaraj.algorithms.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.suvariyaraj.algorithms.SubSectionActivity"
            android:parentActivityName="com.suvariyaraj.algorithms.MainActivity" />
        <activity
            android:name="com.suvariyaraj.algorithms.AlgorithmActivity"
            android:label="@string/title_activity_algorithm"
            android:parentActivityName="com.suvariyaraj.algorithms.SubSectionActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.suvariyaraj.algorithms.DoubtActivity"
            android:parentActivityName="com.suvariyaraj.algorithms.AlgorithmActivity"
            android:theme="@android:style/Theme.Dialog" />
        <activity
            android:name="com.suvariyaraj.algorithms.MyIntro"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.suvariyaraj.algorithms.ImageFullScreenActivity" />
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name="com.suvariyaraj.algorithms.AboutUs" />
    </application>

</manifest>