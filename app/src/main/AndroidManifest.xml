<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.suvariyaraj.algorithms">
<uses-permission android:name="android.permission.VIBRATE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon3"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SubSectionActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AlgorithmActivity"
            android:label="@string/title_activity_algorithm"
            android:parentActivityName=".SubSectionActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DoubtActivity"
            android:parentActivityName=".AlgorithmActivity"
            android:theme="@android:style/Theme.Dialog" />
        <activity
            android:name=".MyIntro"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity android:name=".ImageFullScreenActivity" />
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".AboutUs"></activity>
    </application>

</manifest>